docker push bobykus/dummy-pdf-or-png-proxy:1.0.1

kubectl get pods | awk '{print "kubectl delete pod "$1}'

kubectl delete service dummy-pdf-or-png-proxy-service
kubectl delete deployment  dummy-pdf-or-png-proxy
minikube stop
minikube delete
minikube start

kubectl apply -f k8s-sre-deployment.yml

